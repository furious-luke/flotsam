{
  "name": "flotsam",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "storybook": "start-storybook",
    "test": "jest",
    "docs": "typedoc --jsx react --out docs src",
    "build": "yarn clean && yarn copy",
    "clean": "rm -rf dist",
    "copy": "yarn copy:src && yarn copy:pkg",
    "copy:src": "babel src --out-dir dist --copy-files --no-copy-ignored",
    "copy:pkg": "cp package.json dist/"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.1",
    "@storybook/react": "^5.3.19",
    "babel-loader": "^8.1.0",
    "jest": "^26.0.1",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "@reach/router": "^1.3.3",
    "@types/react": "^15.0.0 || ^16.0.0",
    "baseui": "^9.74.0",
    "flatted": "^2.0.1",
    "fuse.js": "^3.4.6",
    "react": "^16.13.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.12.18",
    "styletron-engine-atomic": "^1.4.4",
    "styletron-react": "^5.2.6"
  },
  "dependencies": {},
  "jest": {
    "moduleNameMapper": {
      "^flotsam(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "./src/setup-tests.js"
    ]
  },
  "description": "A collection of handy JavaScript utilities geared towards building UIs with Uber's BaseWeb.",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furious-luke/flotsam.git"
  },
  "author": "Luke Hodkinson <furious.luke@gmail.com>",
  "bugs": {
    "url": "https://github.com/furious-luke/flotsam/issues"
  },
  "homepage": "https://github.com/furious-luke/flotsam#readme"
}
